{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "description": "A generator for Yeoman",
  "keywords": [
    "yeoman-generator"
  ],
  "homepage": "<%= githubUrl %>/<%= _.slugify(appname) %>",
  "bugs": "<%= githubUrl %>/<%= _.slugify(appname) %>/issues",
  "author": {
    "name": "<%= realname %>",
    "email": "<%= email %>",
    "url": "<%= githubUrl %>"
  },
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= githubUser %>/<%= _.slugify(appname) %>.git"
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script"
  },
  "dependencies": {
    "yeoman-generator": "~0.13.0",
    "coffee-script": "~1.6.3"
  },
  "devDependencies": {
    "mocha": "~1.12.0"
  },
  "peerDependencies": {
    "yo": ">=1.0.0-rc.1"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
